{
    "version": 2,
    "projects": [
      {
        "src": "backend/",
        "use": "@vercel/node"
      }
    ],
    "builds": [
      {
        "src": "backend/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/backend/$1"
      }
    ]
}
